{
  "kind": "collectionType",
  "collectionName": "jobs",
  "info": {
    "singularName": "job",
    "pluralName": "jobs",
    "displayName": "Job"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "contract": {
      "type": "string",
      "required": true
    },
    "job_name": {
      "type": "string",
      "required": true
    },
    "salary": {
      "required": true,
      "type": "float"
    },
    "schedule": {
      "type": "time",
      "required": true
    },
    "starting_date": {
      "type": "date",
      "required": true
    },
    "finishing_date": {
      "required": true,
      "type": "date"
    },
    "announce": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::announce.announce",
      "inversedBy": "job"
    }
  }
}
